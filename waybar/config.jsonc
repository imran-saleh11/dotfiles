        "spacing": 10,
        "icons": {}
    },
    "bluetooth": {
        "format": "Ôäî",
	"tooltip": false,
        "format-disabled": "Û∞Ç≤",
        "format-connected": "Ôäî",
        "on-click": "blueberry"
    },

    "custom/mydate": {
  "format": "{}",
  "exec": "date '+%a %b %d'",
  "interval": 60,
  "tooltip": false
},

    "clock": {
        "timezone": "Bangladesh/Dhaka",
        "format": "{:%I:%M %p}",
	"tooltip": false,
    "on-click": "gnome-clocks"
    },
    "memory": {
        "format": "{}% ÔÉâ"
    },
    "network": {
        "format": "Ó¨Å",
        "format-wifi": "Ôá´",
        "format-ethernet": "Óºâ",
