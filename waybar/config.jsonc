{
    "layer": "top",
    "position": "top",
    "height": 22,
    "spacing": 4,
    "modules-left": [
        "custom/logo" 
    ],
    "modules-center": [
	 "hyprland/workspaces"
            ],
    "modules-right": [
        "tray",
        "network",
        "pulseaudio",
        "bluetooth",
	"custom/mydate",
	"clock",
	"custom/notification"

    ],
      "custom/notification": {
    "tooltip": true,
    "format": "<span size='10pt'>{icon}</span>",
    "format-icons": {
      "notification": "Û±Ö´",
      "none": "Û∞Çú",
      "dnd-notification": "Û∞Ç†",
      "dnd-none": "Û∞™ì",
